.positionR {
  position: relative;
}

.outAbou {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
}

.outAbou::after {
  width: 1px;
  height: 100%;
  display: block;
  content: '';
  background: #000;
  border-radius: 0 10px 10px 0;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%) rotate(-45deg);
}

.outAbou::before {
  width: 1px;
  height: 100%;
  display: block;
  content: '';
  background: #000;
  border-radius: 0 10px 10px 0;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%) rotate(45deg);
}

