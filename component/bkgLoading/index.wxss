/* component/bkgLoading/index.wxss */
.loading2{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #fff;
}

.loading2 .loading-pic {
  left: 0;
  right: 0;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 200rpx;
  height: 200rpx;
  background: transparent url('data:image/gif;base64,') 50%;
  background-size: contain;
  -webkit-transition: background infinite;
  -webkit-transform: translateY(-60rpx);
}
.heightX {
  top:30%;
}
.loading2 .loading-pic:after {
  content: attr(data-text);
  position: absolute;
  width: 200rpx;
  text-align: center;
  color: #4b4b4b;
  font-size: 28rpx;
  top: 100%;
  left: 50%;
  -webkit-transform: translate(-50%, 20rpx);
}
