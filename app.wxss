/**app.wxss**/
@import "/font/DIN-Medium.wxss";
.clear{zoom:1;clear:both}
.clear:after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0;overflow:hidden}
.elps { overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.elps-line2 { overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; }
.fl { float:left; }
.fr { float:right; }
.cutline{width: 100%;height: 20rpx;background: #EFEFEF;}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page {
  width: 100vw;
  min-height: 100%;
  box-sizing: border-box;
  font-family: Heiti SC, Arial, Lucida Grande, Verdana, STXhei, Microsoft YaHei, hei;
  font-size: 24rpx;
  background: #fff;
  overflow-x:hidden; 
}

/*
image {
  vertical-align: middle;
}
*/

.mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 90%;
  z-index: 222;
  background: rgba(0, 0, 0, 0.5);
}

.admin {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
}

.mac-marbot {
  margin-bottom: 120rpx;
}

.module-content {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
}

.module-list {
  width: 100%;
  height: 170rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e2e2e2;
}

.module-list .module-list-info {
  width: 45%;
  height: auto;
}

.module-list .module-list-info .name, .module-list .module-list-info .time {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.module-list .module-list-info .name {
  font-size: 28rpx;
  color: #000;
}

.module-list .module-list-info .name text {
  padding-right: 7rpx;
}

.module-list .module-list-info .time {
  font-size: 20rpx;
  color: #999;
}

.module-list .module-list-info .time text {
  padding-left: 7rpx;
}

.module-list .module-list-info .no {
  font-size: 20rpx;
  color: #000;
}

.module-list .module-list-info .no text {
  padding-left: 7rpx;
  color: #999;
}

.module-list .module-list-amount {
  width: 50%;
  /* width: calc(100% - 80rpx - 45%); */
  text-align: right;
}

.module-list .module-list-amount .purch, .module-list .module-list-amount .money {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.module-list .module-list-amount .purch {
  font-size: 20rpx;
  color: #999;
}

.module-list .module-list-amount .money {
  font-size: 34rpx;
  color: #000;
}

/*商品列表*/

.product-list {
  width: 100%;
  height: auto;
  background: #fff;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}

.product-list .product-list-a {
  width: 100%;
  height: 100rpx;
  padding: 0 20rpx 0 54rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e2e2e2;
  display: flex;
  align-items: center;
}

.product-list .product-list-a .a-left {
  /* width: 314rpx; */
  width: 288rpx;
  font-size: 26rpx;
  color: #000;
}

.product-list .product-list-a .a-right {
  /* width: 40%; */
  width: calc(100% - 288rpx);
  display: flex;
  justify-content: space-between;
}

.product-list .product-list-a .a-right .quantity {
  font-size: 26rpx;
  color: #000;
}
.product-list .product-list-a .a-right .quantity.num{
  width: 240rpx;
  text-align: center;
}

.product-list .product-list-b {
  width: 100%;
  height: auto;
  padding: 20rpx 0;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.product-list .product-list-b .b-all {
  width: 100%;
  height: 140rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.product-list .product-list-b .b-all.choose {
  background: #f8f8f9;
}

.product-list .product-list-b .b-all .b-left {
  /* width: 49%; */
  width: 322rpx;
  display: flex;
  align-items: center;
}

.product-list .product-list-b .b-all .b-left .img {
  width: 110rpx;
  height: 110rpx;
  margin-right: 10rpx;
}

.product-list .product-list-b .b-all .b-left .img image {
  width: 100%;
  height: 100%;
}

.product-list .product-list-b .b-all .b-left .name {
  width: 60%;
}

.product-list .product-list-b .b-all .b-left .name .n1 {
  font-size: 24rpx;
  color: #000;
}

.product-list .product-list-b .b-all .b-left .name .n2 {
  font-size: 20rpx;
  color: #000;
}

.product-list .product-list-b .b-all .b-left .name .n1,
.product-list .product-list-b .b-all .b-left .name .n2 {
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.product-list .product-list-b .b-all .b-right {
  /* width: 51%; */
  width: calc(100% - 322rpx);
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.product-list .product-list-b .b-all .b-right .number {
  width: 240rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

.product-list .product-list-b .b-all .b-right .number .n1 {
  width: 90rpx;
  overflow: hidden;
  font-size: 24rpx;
  color: #000;
  text-align: center;
  display: inline-block;
}

.product-list .product-list-b .b-all .b-right .number .n4 {
  width: 100rpx;
  padding: 0;
}

.product-list .product-list-b .b-all .b-right .number .n3 {
  width: 40rpx;
  height: 40rpx;
  background: url("data:image/svg+xml;base64,") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  /* top: -4rpx; */
  top: 50%;
  margin-top: -20rpx;
  right: 50%;
  margin-right: -20rpx;
  z-index: 1;
}
.product-list .product-list-b .b-all .b-right .number .btn-wrap{
  width: 80rpx;
  height: 100%;
  position: relative;
}
.product-list .product-list-b .b-all .b-right .number .n2 {
  width: 40rpx;
  height: 40rpx;
  background: url("data:image/svg+xml;base64,") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  /* top: -4rpx; */
  top: 50%;
  margin-top: -20rpx;
  left: 50%;
  margin-left: -20rpx;
  z-index: 1;
}

.product-list .product-list-b .b-all .b-right .number .n5 {
  width: 40rpx;
  height: 40rpx;
  background: url("data:image/svg+xml;base64,") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: -4rpx;
  left: 0;
  z-index: 1;
}

.product-list .product-list-b .b-all .b-right .number .n6 {
  width: 40rpx;
  height: 40rpx;
  background: url("data:image/svg+xml;base64,") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: -4rpx;
  right: 0;
  z-index: 1;
}

.product-list .product-list-b .b-all .b-right .price {
  font-size: 24rpx;
  color: #000;
}

.product-list .product-list-c {
  width: 100%;
  padding: 40rpx 20rpx 40rpx 0;
  box-sizing: border-box;
  border-top: 2rpx dashed #e2e2e2;
  border-bottom: 2rpx dashed #e2e2e2;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.product-list .product-list-c .right-side {
  /* width: 272rpx; */
  width: 320rpx;
  line-height: 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 40rpx;
}

.product-list .product-list-c .right-side:last-child {
  margin-bottom: 0;
}

.product-list .product-list-c .right-side .side-1 {
  font-size: 24rpx;
  color: #000;
  display: flex;
  justify-content: flex-end;
  position: relative;
  align-items: center;
}
.product-list .product-list-c .right-side.back_money .right-arrow{
  margin-left: 10rpx;
  /* position: absolute;
  right: 0;
  top: 50%;
  margin-top: -12rpx; */
}
  /*  */
.product-list .product-list-c .right-side .side-1:first-child {
  width: 155rpx;
}
.product-list .product-list-c .right-side .side-1:last-child {
  /* padding-right: 22rpx; */
}
.product-list .product-list-c .right-side .side-2 {
  width: 96rpx;
  justify-content: flex-end;
}

.product-list .product-list-c .right-side .side-1 .icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 16rpx;
  display: inline-block;
}

.product-list .product-list-c .right-side .side-1 .icon image {
  width: 100%;
  height: 100%;
}

.product-list .product-list-d {
  width: 100%;
  padding: 40rpx 20rpx 30rpx 0;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.product-list .product-list-d .net-side {
  min-width: 272rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.product-list .product-list-d .net-side .net-1 {
  width: 80rpx;
  text-align: center;
  font-size: 24rpx;
  color: #666;
}

.product-list .product-list-d .net-side .net-2 {
  font-size: 34rpx;
  color: #000;
}

/*商品列表end*/

/*店铺招商*/

.shop-merchants {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
}

.shop-steps {
  width: 100%;
  background: #fff;
  padding-top: 46rpx;
  padding-bottom: 20rpx;
}

.steps-text-a {
  color: #4DA1FF;
  font-size: 20rpx;
}

.steps-text-b {
  color: #999;
  font-size: 20rpx;
}

/*店铺招商end*/


/*跑马灯*/

.set-top {
  width: 100%;
  height: 70rpx;
  background: #EDF6FF;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.set-top .marquee {
  /*width: 50%;*/
  display: flex;
}

.set-top .marquee .icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
}

.set-top .marquee .icon image {
  width: 100%;
  height: 100%;
}

.set-top .marquee .text-hint {
  font-size: 20rpx;
  color: #4DA1FF;
}

/*跑马灯end*/

/* 技术支持 */
.FAE {
  width: 100%;
  font-size: 20rpx;
  color: #A8A8A8;
  text-align: center;
  position: fixed;
  left: 0;
  bottom: 36rpx;
}
/* 右侧小箭头 */
.right-arrow {
  width: 13rpx;
  height: 24rpx;
  background: no-repeat center center url('data:image/svg+xml;base64,');
  background-size: cover;
}
.right-clear {
  width: 30rpx;
  height: 30rpx;
  background: no-repeat center center url('data:image/svg+xml;base64,');
  background-size: cover;
}


/* 按钮加载 */
.btn-loading-inner {
  display: inline-block;
  margin-right: 12px;
  vertical-align: middle;
  width: 14px;
  height: 14px;
  background: 0 0;
  border-radius: 50%;
  border: 2px solid #fff;
  border-color:  #fff #fff #fff transparent;
  animation: btn-spin 0.6s linear;
  animation-iteration-count: infinite;
}
.border-color{
  border-color:  #4da1ff #4da1ff #4da1ff transparent;
  animation: btn-spin 0.6s linear;
  animation-iteration-count: infinite;
}
@keyframes btn-spin{0%{transform:rotate(0)}100%{transform:rotate(360deg)}}

