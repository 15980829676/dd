@import "/pages/home/home.wxss";


.animationTop2{
  /* transition: top 0.5s 20px; */
  animation: top2 0.5s linear;
  animation-fill-mode: forwards;
 
}
.rightOrder {
  display: flex;
  align-items: center;
  justify-content: center;
}
.orderIndex {
  font-size:20rpx;
  color:#999999;
  display: flex;
  align-items: center;
  margin-right: 30rpx;
}
.orderIndex.action{
  margin-right: 10rpx;
}
.colorAction {
  font-size:24rpx;
color:#333333;

}
@keyframes top2{
  0%{top: 40rpx;}
  100%{top: 0rpx;}
}

