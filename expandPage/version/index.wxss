/* expandPage/version/index.wxss */
.box{
  width: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.box .item{
  height: 160rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 0 40rpx;
  border-bottom: 2rpx solid #E2E2E2;
}
.box .item .item-l{
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
  color: #333333;
}
.box .item .item-l .time{
  margin-top: 15rpx;
  font-size: 20rpx;
  color: #999999;
}