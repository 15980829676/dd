/* pages/personal information/personal information.wxss */
.empty{
  width: 100%;
  text-align: center;
  color: #a9abb1;
}
.progress-item{
  position: relative;
}
.progress-item .hint{
  position: absolute;
  right: -60rpx;
  top: 0;
  height: 100%;
}
.mac-marbot {
  /* margin-bottom: 110rpx; */
  margin-bottom: 0;
}
page {
  background: #efefef;
}
.module-list {
  background: white;
}
.pi-head {
  width: 100%;
  height: 440rpx;
  overflow: hidden;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
  
}
.fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1000;
}

.pi-head .head-t {
  width: 100%;
  height: 280rpx;
  padding: 0 18rpx 0 44rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.pi-head .head-t .head-t-left {
  width: 110rpx;
  height: auto;
  margin-right: 36rpx;
}

.pi-head .head-t .head-t-left .via {
  width: 110rpx;
  height: 110rpx;
  border-radius: 100%;
  margin-right: 36rpx;
  overflow: hidden;
}

.pi-head .head-t .head-t-left .via image {
  width: 100%;
  height: 100%;
}

.pi-head .head-t .right-side {
  width: calc( 100% - 146rpx);
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex: 1;
}

.pi-head .head-t .head-t-middle {
  width: 110rpx;
  /*margin-right: 60rpx;*/
}

.pi-head .head-t .head-t-middle .name {
  width: 100%;
  height: 50rpx;
  font-size: 36rpx;
  color: #fff;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-bottom: 6rpx;
}

.pi-head .head-t .head-t-middle .rank {
  width: 100rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: #4da1ff;
  border-radius: 5rpx;
  font-size: 18rpx;
  color: #fff;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.pi-head .head-t .head-t-right {
  width: 80%;
  /*height: auto;*/
  display: flex;
  flex-direction: column;
  color: #fff;
  padding-top: 15rpx;
}

.pi-head .head-t .head-t-right .hhh {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 24rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 8rpx;
}
.pi-head .head-t .head-t-right .hhh .hhh-text{
  max-width: 85%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow:hidden;
}

.pi-head .head-t .head-t-right .hhh:first-child{margin-top: 0;}

.pi-head .head-t .head-t-right .hhh .icon {
  width: 22rpx;
  height: 26rpx;
  padding: 0 14rpx;
  display: inline-block;
}

.pi-head .head-t .head-t-right .hhh .icon1 {
  width: 32rpx;
}

.pi-head .head-t .head-t-right .hhh .icon2 {
  width: 26rpx;
}

.pi-head .head-t .head-t-right .hhh .icon image {
  width: 100%;
  height: 100%;
}

.pi-head .head-b {
  width: 100%;
  height: 160rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.pi-head .head-b .head-b-left {
  width: 56%;
}

.pi-head .head-b .head-b-left .hint {
  display:flex;
  align-items:center;
  font-size: 20rpx;
  white-space: nowrap;
  color: #fff;
}

.pi-head .head-b .head-b-left .hint text {
  padding: 0 7rpx;
}

.pi-head .head-b .head-b-right {
  width: 44%;
  text-align: right;
}

.pi-head .head-b .head-b-right .money {
  font-size: 34rpx;
  color: #fff;
}

.pi-head .head-b .head-b-right .sum {
  font-size: 20rpx;
  color: #fff;
}

.module-list .module-list-info {
  width:50%;
  height:auto;
}

