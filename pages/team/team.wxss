.i-index-demo {
  /* padding-bottom: 90rpx; */
}
page{
  /* background: #eee !important; */
}
.share {
  position: fixed;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  border-radius: 0;
  font-size: 24rpx;
  z-index: 1500;
  padding: 0;
  color: #fff;
  overflow: hidden;
  transition-duration: 0.3s;
  margin: 0;
  border-color: transparent !important;
  border: 0 !important;
  /* box-shadow: 0 0 8rpx rgba(0, 0, 0, 0.14), 0 8rpx 16rpx rgba(0, 0, 0, 0.28); */
}
.share .wrap{
  width: 100%;
  height: 100%;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  -ms-flex-pack: center;
  justify-content: center;
  background-color: #4da1ff;
  font-size: 28rpx;
  color: #fff;
}
.icon-share{
  box-sizing: border-box;
  width: 40rpx;
  height: 40rpx;
  background: url("data:image/svg+xml;base64,") no-repeat center center;
  background-size: 40rpx 40rpx;
  margin-right: 5rpx;
}
/* 吸顶 */

.scroll-title {
  width: 100%;
  height: 60rpx;
}

.scroll-title-text {
  width: 100%;
  padding: 0 24rpx;
  box-sizing: border-box;
  line-height: 60rpx;
  font-weight: 400;
  background-color: #eee;
}

.border-bot {
  border-bottom: 2rpx solid #efefef;
  background-color: #fff;
}

.title-fixed {
  position: fixed;
  top: 100rpx;
}

/* 右侧索引 */

.scroll-index {
  position: fixed;
  right: 0;
  top: 50%;
  z-index: 10;
  padding-left: 10px;
  transform: translateY(-50%);
}

.scroll-index-item {
  display: block;
  height: 18px;
  line-height: 18px;
  padding: 0 5px;
  text-align: center;
  color: #2d8cf0;
  font-size: 12px;
  border-radius: 50%;
}

.currentKey {
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate3d(-50%, -50%, 0);
  background: rgba(0, 0, 0, 0.7);
  color: #fff;
  font-size: 48rpx;
  border-radius: 50%;
  width: 160rpx;
  height: 160rpx;
  line-height: 160rpx;
  text-align: center;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* // */

.orderList-box {
  padding-top: 100rpx;
  padding-bottom:90rpx; 
  height: 100vh;
  box-sizing: border-box;
  /* background-color:#eee; */

}

.orderList-wrap {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1000;
}

/* 当前查询的成员 */

.curr-p {
  position: relative;
  width: 100%;
  height: 80rpx;
  border-bottom: 1rpx solid #e2e2e2;
  box-sizing: border-box;
  line-height: 80rpx;
  padding: 0 20rpx;
  background-color: rgba(237, 246, 255, 1);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.curr-p .btn {
  display: flex;
  align-items: center;
}

.curr-p .btn .icon {
  width: 30rpx;
  height: 30rpx;
}

.curr-p .btn .text {
  font-size: 20rpx;
  margin-left: 18rpx;
  color: #4da1ff;
}

/* 成员空 */

.member-null {
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #999;
}

/* 邀请新成员 */

.addempbtn {
  bottom: 0;
  position: fixed;
  z-index: 100;
  width: 100%;
}

.commit-button {
  width: 100%;
  height: 86rpx;
  background: #4da1ff;
  font-size: 24rpx;
  outline: none;
  border-radius: 0;
  border: 0;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.addmemberpic {
  width: 34rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.btn-text {
  font-size: 28rpx;
  letter-spacing: 4rpx;
  color: #fff;
}

/* 列表item */

.team-list {
  width: 100%;
  height: 170rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 90rpx 0 50rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e2e2e2;
  background-color: #fff;
}

.team-list .team-list-head {
  width: 110rpx;
  height: 110rpx;
  border-radius: 100%;
  overflow: hidden;
  margin-right: 30rpx;
}

.team-list .team-list-head image {
  width: 100%;
  height: 100%;
}

.team-list .team-list-all {
  width: 77%;
  height: auto;
  display: flex;
  justify-content: space-between;
}

.team-list .team-list-info {
  width: 45%;
  height: auto;
}

.team-list .team-list-info .name, .team-list .team-list-info .time {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.team-list .team-list-info .name {
  min-height: 38rpx;
  font-size: 28rpx;
  color: #000;
}

.team-list .team-list-info .time {
  font-size: 20rpx;
  color: #999;
}

.team-list .team-list-info .time text {
  padding-left: 7rpx;
}

.team-list .team-list-amount {
  width: 55%;
  text-align: right;
}

.team-list .team-list-amount .purch, .team-list .team-list-amount .money {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.team-list .team-list-amount .purch {
  font-size: 20rpx;
  color: #999;
}

.team-list .team-list-amount .money {
  font-size: 34rpx;
  color: #000;
}

/* 没数据 */

.tm-f {
  width: 100%;
  overflow: hidden;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.tm-f .img {
  width: 100%;
  margin-top: 150rpx;
  /* margin-bottom: 170rpx; */
}

.tm-f .img image {
  width: 100%;
  height: 100%;
}

.tm-f .hint {
  width: 100%;
  font-size: 28rpx;
  color: #333;
  letter-spacing: 6rpx;
  text-align: center;
}

.tm-f .btn {
  width: 170rpx;
  height: 64rpx;
  line-height: 64rpx;
  background: #4da1ff;
  border: none;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  border-radius: 10rpx;
  letter-spacing: 2rpx;
  margin-top: 72rpx;
  margin-bottom: 54rpx;
  padding: 0;
}

.tm-f .reminder {
  width: 100%;
  font-size: 24rpx;
  color: #999;
  text-align: center;
  letter-spacing: 2rpx;
}
