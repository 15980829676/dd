/* pages/marketing tool/marketing tool.wxss */
page{background: #EFEFEF;}
.admin {
 box-sizing: border-box;
 padding: 0 16rpx;
}

.mark-t{margin-bottom: 0;background: white;border-radius:15rpx;overflow: hidden;margin-top: 20rpx;}
.headText {
  font-size: 14px;
  color: #706f75;
  height: 96rpx;
  line-height: 96rpx;
  padding-left: 40rpx;
  box-sizing: border-box;
  border-left:1rpx solid #e9eaec;
  border-right:1rpx solid #e9eaec;
  border-bottom:1rpx solid #e9eaec;

}

.carousel {
  width: 100%;
  position: relative;
  overflow: hidden;
  border-radius:15rpx;
  margin-top: 20rpx; 
}

.carousel .viewpager {
  width: 100%;
  height: 300rpx;
}

.carousel .viewpager image {
  width: 100%;
  height: 100%;
}
.width33 {
  width:33.33333333%;
  display: block;
}