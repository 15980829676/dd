@keyframes swing {
    0%,10%,20%{
       -webkit-transform: rotate(0);
        transform: rotate(0)
    }
    
    30% {
        -webkit-transform: rotate(2deg);
        transform: rotate(2deg)
    }

    50% {
        -webkit-transform: rotate(-2deg);
        transform: rotate(-2deg)
    }

    70% {
        -webkit-transform: rotate(1deg);
        transform: rotate(1deg)
    }

    90% {
        -webkit-transform: rotate(-1deg);
        transform: rotate(-1deg)
    }

    to {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }
}
