.againOrder .box {
  width: 640rpx;
  /* height: 440rpx; */
  overflow: hidden;
  border-radius: 8rpx;
  background: #fff;
  position: relative;
}

.againOrder .box .title {
  display: flex;
  flex-direction: column;
  width: 100%;
  text-align: center;
}
.againOrder .box .title .tit{
  font-size: 36rpx;
  color: #333;
  padding: 50rpx 0 40rpx;
  font-weight: bold;
}
.againOrder .box .title .text{
  font-size: 24rpx;
  color: #999;
}
.againOrder .box .goods{
  width: 100%;
  padding: 30rpx 30rpx 0;
  box-sizing: border-box;
  max-height: 500rpx;
  margin-bottom: 10rpx;
  overflow-y: auto;
}
.againOrder .box .goods .item {
  padding-bottom: 45rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  color: #333;
  /* background: #eee; */
}
.againOrder .box .goods .item .g-l {
  
}
.againOrder .box .goods .item .g-r {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.againOrder .box .goods .item .g-r .num {

}
.againOrder .box .goods .item .g-r .status {
  width: 110rpx;
  padding-right: 10rpx;
  box-sizing: border-box;
  text-align: right;
  color: #999;
}
.againOrder .box .btn-wrap{
  width: 100%;
  padding: 0 30rpx 40rpx;
  box-sizing: border-box;
}
.againOrder .box .btn-wrap .btn {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx !important;
  border-radius: 8rpx !important;
  background: #4da1ff !important;
  color: #fff !important;
  margin: 0 !important;
  box-shadow: none;
}
.againOrder .box .btn-wrap .btn::before {
  content: '';
  border: 0 !important;
  width: 0;
  display: block;
}
.againOrder .box .btn-wrap .btn::after {
  content: '';
  border: 0 !important;
   width: 0;
  display: block;
}
.againOrder .box .btn-wrap .btn-hover {
  opacity:.9;
}
.border{border:none !important;}
/*活动助手机器*/
.activityAssistant {
  width: 100%;
  height:109rpx;
  padding-bottom:20px; 
}
.activityImage {
  width:100rpx;
  height:109rpx;
  position:fixed;
  background-repeat: no-repeat; 
  bottom:70px;
  left:29rpx;
  z-index:3;
  background-size:contain; 
}
.assistantTop {
  width:100%;
  height: 100%;
  position: fixed;
  box-sizing: border-box;
  padding-bottom: 50px;
  display: flex; 
  top: 0;
  left: 0;
  z-index:102;
  flex-direction: column-reverse;
  background: rgba(0,0,0,0.5);
}
.assistantRow {
  width: 100%;

  background: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  transform: translateY(100%)
}
.assistantHead {
  width: 100%;
  height: 50px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0 20rpx;

}
.assOut {
  width: 40rpx;
  height: 40rpx;
}
.widthImage {
  width: 100%;
  height: 100%;
}
.assistantScroll {

  box-sizing: border-box;
  padding: 0 20rpx;

  background: #ccc;
}