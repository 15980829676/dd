/* pages/shopMerchants/info.wxss */
@import "/wxParse/wxParse.wxss";
page {
  background: #f9f9f9;
}
#agreement {
  width: 100%;
  background: #fff;
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  z-index: 91;
  min-height: 960rpx;
  overflow: scroll;
  padding-top:30rpx;
 
}

#agreement .agreement-text {
  padding: 60rpx 30rpx 50rpx;
  padding-bottom:0;
  box-sizing: border-box;
}

.top-bar .close-agreement {
  display: inline-block;
  float: right;
  position: absolute;
  top: 24rpx;
  right: 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 30rpx 30rpx;
  background-position:center right;
  width: 40rpx;
  height: 40rpx;
}
.top-bar {
  padding: 25rpx 20rpx;
  text-align: center;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 777;
  height: 100rpx;
  line-height: 50rpx;
  box-sizing: border-box;
  background: #238787;
}

.top-bar .bk {
  position: absolute;
  top: 0;
  width: 60rpx;
  height: 100%;
  left: 20rpx;
  text-align:left;
}

.top-bar .bk image {
  position:relative;
  top:24rpx;
  width: 18rpx;
  height: 32rpx;
}

.tit {
  position: relative;
  font-size: 32rpx;
  color: #fff;
}

.shop-merchants {
  /* padding-top: 100rpx; */
}
.shop-wrap{width:100%;height:auto; float:left; margin-bottom:120rpx;}
.shop-store {
  width: 100%;
  background: #fff;
  padding: 36rpx 20rpx 30rpx;
  box-sizing: border-box;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

.shop-store .incl {
  width: 100%;

  justify-content: space-between;
}

.shop-store .incl .grade {
  width: 49%;
  height: 142rpx;
  border: 2rpx solid #e5e5e5;
  padding-left: 24rpx;
  border-radius: 16rpx;
  margin-right: 2%;  float:left;
  position: relative;
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

.shop-store .incl .grade:nth-child(2n) {
  margin-right: 0;
}

.shop-store .incl .grade.select {
  border: 2rpx solid #4DA1FF;
}

.shop-store .incl .grade.select::after {
  content: "";
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  top: 0;
  right: 0;
  background: url("data:image/svg+xml;base64,") no-repeat;
  background-size: 100% 100%;
}

.shop-store .incl .grade.gais::after {
  content: "";
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  top: 0;
  right: 0;
  background: url("data:image/svg+xml;base64,") no-repeat;
  background-size: 100% 100%;
}



.shop-store .incl .grade .v1 {
  width: 100%;
  margin-top: 34rpx;
  color: #4DA1FF;
  font-size: 24rpx;
}

.shop-store .incl .grade .v2 {
  width: 100%;
  margin-top: 8rpx;
  color: #666;
  font-size: 28rpx;
}

.shop-center {
  width: 100%;
  background: #fff;
  margin-top: 20rpx;
  padding: 36rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.fill-title {
  font-size: 32rpx;
  letter-spacing: 4rpx;
  color: #333;
  font-weight: bold;
  width: 100%;
  text-align: center;
  padding-bottom: 40rpx;
}

.user {
  width: 100%;
  height: 98rpx;
  border-bottom: 2rpx solid #e2e2e2;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.user .wan {
  width: 166rpx;
  letter-spacing: 4rpx;
  font-size: 24rpx;
  font-weight: bold;
  color: #333;
}

.user .wan.disn {
  display: none;
}

.user input, .user textarea {
  width: 70%;
  color: #999;
  border: none;
  letter-spacing: 4rpx;
  font-size: 24rpx;
  background: transparent;
  vertical-align: middle;
}

.hgt26 {
  height: 50rpx;
  overflow: hidden;
  top:10rpx;
  /*padding-top: 8rpx;*/
}

.position_rel {
  position: relative;
}

.checkCode {
  display: inline-block;
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 0;
  height: 52rpx;
  line-height: 52rpx;
  width: 208rpx;
  text-align: center;
  color: #fff;
  background:#4DA1FF;
  border-radius: 10rpx;
}

.hint {
  font-size: 20rpx;
  letter-spacing: 2rpx;
  color: #4DA1FF;
  width: 100%;
  padding-top: 36rpx;
  padding-bottom: 20rpx;
}

.down-icon {
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  right: 40rpx;
  width: 16rpx;
  height: 8rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
}

.agreement {
  width: 100%;
  padding: 46rpx 0 32rpx;
  position: relative;
}

.agreement .rig {
  width: 100%;
  padding-left: 52rpx;
  box-sizing: border-box;
}

.agreement .rig text {
  color: #999;
  font-size: 20rpx;
}

.agreement .fds {
  margin-right: 10rpx;
  position: absolute;
  top: 46rpx;
  left: 0;
}

.check {
  background-image: url(data:image/svg+xml;base64,);
}

.check, .check-on {
  width: 34rpx;
  height: 34rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

.check-on {
  background-image: url(data:image/svg+xml;base64,);
}

.agreement .rig .olm {
  display: inline;
  color: #4DA1FF;
  font-size: 20rpx;
  line-height: 34rpx;
}

.payment {
  width: 100%;
  height: 248rpx;
  padding: 140rpx 92rpx 50rpx;
  box-sizing: border-box;
  text-align: center;
  color: #999;
  font-size: 24rpx;
}

.pay-btn {
  position:fixed;
  bottom:0;
  left:0;
  width: 100%;
  height: 94rpx;
  background: #4DA1FF;
  line-height: 94rpx;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  border-radius: 4rpx;
  z-index:44;
}
.shop-wraps{
	width:100%;
	height:auto;
	float:left; 
	
}

.shop-audit {
  width: 100%;
  padding: 80rpx 30rpx 174rpx;
  text-align: center;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-direction:column;
}

.shop-audit .audit-icon {
  width: 112rpx;
  height: 120rpx;
}

.shop-audit .audit-icon image {
  width: 100%;
  height: 100%;
}

.shop-audit .audit-being {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
  padding: 36rpx 0 20rpx;
}

.shop-audit .audit-character {
  font-size: 28rpx;
  color: #999;
}

.shop-audit .audit-tel {
  font-size: 24rpx;
  color: #999;
  padding-top: 290rpx;
  padding-bottom: 46rpx;
}

.shop-audit .audit-btn {
  width: 100%;
  height: 94rpx;
  line-height: 94rpx;
  
}
.audit-btns{width: 100%;
  height: 94rpx;
  line-height: 94rpx;
  background: #4DA1FF;
  border-radius: 4rpx;
  font-size: 32rpx;
  color: #fff;
  letter-spacing: 4rpx;}
.btn-nei{width: 100%;
  height: 94rpx;
  line-height: 94rpx;
  background: #4DA1FF;
  border-radius: 4rpx;
  font-size: 32rpx;
  color: #fff;
  letter-spacing: 4rpx;}
.shop-merchants.padt{padding-top:0;}
.wxParse-inline{font-size:24rpx; line-height:1.3;}
picker{
	width:70%;
	height:60rpx;
	line-height:66rpx;
}
.i-input-input{width:70%; height:80rpx; line-height:80rpx;}

.btn-box{
  width:100%; 
  height:100%;
}
.top-bar{background: #4DA1FF !important;}
.btn-left,.btn-right{
  width: 49%;
  height: 94rpx;
  line-height: 94rpx;
  background: #4DA1FF;
  border-radius: 10rpx;
  font-size: 28rpx;
  color: #fff;
  letter-spacing: 4rpx;
  text-align:center;
  overflow:hidden;
}
.btn-left{float:left;}
