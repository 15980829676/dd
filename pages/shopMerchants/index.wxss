/* pages/shopMerchants/index.wxss */
.shop-merchants{
  overflow: hidden;
}
.shop-m {
  width: 100%;
  height: 372rpx;
  background: #4da1ff;
  padding: 100rpx 0 68rpx 80rpx;
  box-sizing: border-box;
}

.shop-m .headicon {
  width: 100%;
  height: 82rpx;
  overflow: hidden;
}

.shop-m .headicon image {
  width: 82rpx;
  height: 100%;
  float:left;

}
.shop-m .headicon text{
	width:70%;
	height:auto;
	float:left;
}
.shop-m .headicon .tet {
  line-height:30rpx;
  height:30rpx;
text-overflow:ellipsis;
white-space: nowrap;
  padding-left:20rpx;
  color:#fff;
  float:left;
  width:100%;
  overflow:hidden;
}
.name-s{margin-top:10rpx;}
.shop-m .headagent {
  width: 100%;
  height: 52rpx;
  color: #fff;
  font-size: 38rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 24rpx 0 8rpx;
}

.shop-m .headequity {
  width: 100%;
  height: 38rpx;
  color: #fff;
  font-size: 28rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.shop-marb {
  margin-bottom: 120rpx;
  width:100%;
  height:auto;
  padding-left:80rpx;
  box-sizing: border-box;
  position:relative;
}

.shop-mc {
  width: 200%;
  height: auto;
  box-sizing: border-box;
  position:relative;
}

.vivi {
  width: 100%;
  height: auto;
  padding: 40rpx 0;
  position:relative;
}

.vivi-top {
  width: 100%;
  height: auto;
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  display: flex;
  align-items: center;
  position:absolute;
  top:140rpx;
  left:80rpx;
  z-index:33;
}
.vivi-top.top2{
	top:380rpx;
}
.vivi-top.top3{
	top:620rpx;
}
.vivi-top.top4{
	top:910rpx;
}
.vivi-top .t-icon {
  width: 48rpx;
  height: 48rpx;
  margin-left: 20rpx;
  display: flex;
}

.vivi-top .t-icon image {
  width: 26rpx;
  height: 26rpx;
  margin-top:11rpx;
}

.vivi-line {
  width: 100%;
  height: 2rpx;
  background: #cecece;
  margin: 30rpx 0 40rpx;
}

.vivi-mon {
  width: 160rpx;
  font-size: 28rpx;
  color: #666;
  margin-right: 120rpx;
  display: inline-block;
}

.vivi-mon:last-child {
  margin-right: 0;
}
.vivi-mon.show {
  width: 230rpx;
  margin-right: 30rpx;
  white-space: normal;
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;
overflow: hidden;
float:left;
}
.vivi-mon.show:last-child {
  margin-right: 0rpx;

}
.vivi-text {
  font-size: 28rpx;
  color: #666;
  display: inline-block;
  position:absolute;
  top:740rpx;
  left:80rpx;
  z-index:33;
  padding-right:10rpx;
}

.shop-b {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  z-index:35;
}

.shop-b .lefticon {
  width: 25%;
  height: 100%;
  background: #c1deff;
  display: flex;
  align-items: center;
  justify-content: center;
}

.shop-b .lefticon .service {
  /* width: 40rpx;
  height: 40rpx; */
  width: 100%;
  height: 100%;
  background: none;
  border: 0;
  outline: none;
  margin: 0;
  padding: 0;
  position: relative;
}

.shop-b .lefticon .service image {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 50%;
  margin-left: -20rpx;
  top: 50%;
  margin-top: -20rpx;
}

.shop-b .rightbutton {
  width: 75%;
  height: 100%;
  line-height: 100rpx;
  background: #4da1ff;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
}

.shop-tg {
  width: 100%;
  height: 100rpx;
  background: #367ac7;
  box-sizing: border-box;
  position:absolute;
  top:0;
  left:0;
}

.grade {
  width: 160rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #fff;
  font-weight: bold;
  display: inline-block;
  margin-right: 120rpx;
  overflow: hidden;
  float:left;
}
.grade:last-child {
  margin-right: 0;
}

.Dialog {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 222;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  align-items: center;
  justify-content: center;
}

.Dialog .log-con {
  width: 650rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.Dialog .log-con .content {
  width: 650rpx;
  height: 770rpx;
  padding: 7% 10%;
  box-sizing: border-box;
  overflow: hidden;
  background: #fff;
  border-radius: 10rpx;
}

.Dialog .log-con .content .title {
  font-size: 36rpx;
  color: #343434;
}

.Dialog .log-con .content .line {
  width: 100%;
  height: 3rpx;
  background: #E2E2E2;
  margin: 52rpx 0;
}

.Dialog .log-con .content .text {
  font-size: 24rpx;
  color: #343434;
  line-height: 48rpx;
}

.Dialog .log-con .close {
  width: 75rpx;
  height: 140rpx;
}

.Dialog .log-con .close image {
  width: 100%;
  height: 100%;
}

.i-tabs {
  background: #238787 !important;
}

.i-tabs-tab-title {
  font-size: 36rpx;
  font-weight: bold;
  color: #fff;
}
