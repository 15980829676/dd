.menu-row {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  box-sizing: border-box;
  padding: 30rpx 0;
}
.menu-a-left {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.icon_menu {
  width:35px;
  height:40px;
}
.menu-a {
  width: auto;
  display: flex;

}
.displayCent {
  display: flex;
  justify-content: center;
  align-items: center;
}
.imageWidth {
  width: 35px;
