}
/*提示*/
.reminder {
  width:412rpx;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  right: 7%;
  top: 14px;
  box-shadow: 0 1rpx 4rpx 2rpx rgba(0, 0, 0, 0.1) !important;
  z-index: 2001;

}
.reminder::after {
  width: 28rpx;
  height: 28rpx;
  box-shadow: 0 1rpx 4rpx 2rpx rgba(0, 0, 0, 0.1) !important;
  position: absolute;
  top: 0;
  right: 85rpx;
  transform: translateY(-50%) rotate(45deg);
  background: white;
  display: block;
  content: '';
}
.reminderText {
  width: 100%;
  height: auto;
  position: relative;
  font-size:24rpx;
  color:#333333;
  background: white;
  padding: 22rpx;
  background: white; 
  z-index: 3;
  text-align: center;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.dcenter {
  display: flex;
  align-items: center;
  justify-content: center;
}
.marginlr {
  margin: 0 10rpx;
}
.smd {
  width: 8rpx;
  height: 8rpx;
  border-radius:100%;
  background: black; 
}
.bgd {
width: 13rpx;
  height: 13rpx;
  border-radius:100%;
  background: black;
  margin: 0 3px;
}
.reminderOut {
  width: 30rpx;
  height: 30rpx;
  background: #e5e5e5;
  border-radius:100%;
  position: absolute;
  right:8rpx;
  top:25rpx;
}
.reminderOut::after {
  width: 2px;
  height: 60%;
  position: absolute;
  background: white;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%) rotate(45deg);
  display: block;
  content: '';
}
.reminderOut::before {
  width: 2px;
  height: 60%;
  position: absolute;
  background: white;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%) rotate(-45deg);
  display: block;
  content: '';
}