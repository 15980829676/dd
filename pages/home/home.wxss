/* pages/home/home.wxss */
.top_shadow{
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  height: 10rpx;
  z-index: 10000;
}

.margin-bot{
  margin-bottom: 20rpx;
}
.margin-bot:last-child{
  margin-bottom: 0;
}

page {
  background: #EFEFEF;
}
.hide{
  display: none !important;
}
.Dialog {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 222;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  align-items: center;
  justify-content: center;
}

.Dialog .log-con {
  width: 590rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 10rpx;
  overflow: hidden;
}

.Dialog .log-con .content {
  width: 590rpx;
  padding: 60rpx;
  box-sizing: border-box;
  overflow: hidden;
  background: #fff;
  position: relative;
}

.Dialog .log-con .content.bd-ri {
  border-radius: 10rpx;
}

.Dialog .log-con .content .title {
  font-size: 32rpx;
  color: #1A1311;
  margin-bottom: 14rpx;
}

.Dialog .log-con .content .subtitle {
  font-size: 20rpx;
  color: #999999;
}

.Dialog .log-con .content .line {
  width: 100%;
  height: 3rpx;
  background: #E2E2E2;
  margin: 40rpx 0;
}

.Dialog .log-con .content .text {
  font-size: 24rpx;
  color: #666666;
  line-height: 40rpx;
  margin-bottom: 74rpx;
}

.Dialog .log-con .content .date {
  font-size: 24rpx;
  color: #333333;
}

.Dialog .log-con .content .sign {
  width: 264rpx;
  height: 104rpx;
  position: absolute;
  bottom: 20rpx;
  right: -85rpx;
}

.Dialog .log-con .content .sign image {
  width: 100%;
  height: 100%;
}

.Dialog .log-con .close {
  width: 50rpx;
  height: 120rpx;
}

.Dialog .log-con .close image {
  width: 100%;
  height: 100%;
}

.Dialog .log-con .btn {
  width: 590rpx;
  height: 100rpx;
  background: #4DA1FF;
  display: flex;
  align-items: center;
  justify-content: center;
}

.Dialog .log-con .btn .down {
  width: 170rpx;
  height: 64rpx;
  line-height: 64rpx;
  border-radius: 10rpx;
  overflow: hidden;
  background: #fff;
  text-align: center;
  font-size: 24rpx;
  color: #4DA1FF;
  letter-spacing: 2rpx;
}

.carousel {
  width: 100%;
  position: relative;
  overflow: hidden;
}

.carousel .viewpager {
  width: 100%;
  height: 300rpx;
}

.carousel .viewpager image {
  width: 100%;
  height: 100%;
}

.marquee {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
  position: relative;
  /* margin-bottom: 20rpx; */
}

.marquee .icon {
  width: 40rpx;
  height: 30rpx;
  margin-right: 30rpx;
  position: relative;
}

.marquee .icon image {
  width: 100%;
  height: 100%;
}

.marquee .notice {
  width: 76%;
  height: 100rpx;
  font-size: 24rpx;
  color: #333333;
}
.marquee .notice .notice-item{
  width: 100%;
  height: 100%;
  line-height: 100rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.marquee .more {
  font-size: 24rpx;
  color: #4DA1FF;
  position: absolute;
  right: 30rpx;
  line-height: 100rpx;
}

.backlog {
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
  /* margin-bottom: 20rpx; */
}

.backlog .todo {
  width: 100%;
  height: 100rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #333333;
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #EFEFEF;
}

.backlog .portal {
  width: 100%;
  padding: 36rpx 0 42rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
}

.backlog .portal .por-a{
  width: 100rpx;
}

.backlog .portal .por-a .hint{
  position: relative;
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
  background: #EFEFEF;
  color: #333333;
  margin-bottom: 24rpx;
  overflow: hidden;
  /* display: flex;
  align-items: center; */
}
.backlog-swiper{
  width: 100%;
}
.backlog .portal .por-a .hint.knot {
  background: #4DA1FF;
  color: #fff;
}

.backlog .portal .por-a .hint .number {
  width: 100%;
  height: 40rpx;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  position: absolute;
  top: 35rpx;
  overflow: hidden;
}

.scroll-wrap{
  width: 100%;
  height:auto;
  position: absolute;
  left: 0;
  top: 0;
}

.animationTop{
  /* transition: top 0.5s 20px; */
  animation: top 0.5s linear;
  animation-fill-mode: forwards;
}
@keyframes top{
  0%{top: 0;}
  100%{top: -40rpx;}
}


.scroll-wrap-item {
  width: 100%;
  height:40rpx;
  line-height: 40rpx;
}
.backlog .portal .por-a .text {
  color: #333333;
  font-size: 20rpx;
}

.backlog .portal .por-a .hint .number, .backlog .portal .por-a .text {
  width: 100%;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.incomeexpenses {
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
  /* margin-bottom: 20rpx; */
}

.incomeexpenses .stupid {
  width: 100%;
  height: 100rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 2rpx solid #EFEFEF;
}

.incomeexpenses .stupid .text {
  font-size: 24rpx;
  color: #333333;
}

.incomeexpenses .stupid .advance {
  width: 100rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.incomeexpenses .stupid .advance image {
  width: 100%;
  height: 100%;
}

.incomeexpenses .ie-wrap {
  width: 100%;
  display: flex;
  flex-direction: column;
}

.incomeexpenses .ie-wrap .wrap-a {
  width: 100%;
  display: flex;
  border-bottom: 2rpx solid #EFEFEF;
}

.incomeexpenses .ie-wrap .wrap-a:last-child {
  border-bottom: 0;
}

.incomeexpenses .ie-wrap .wrap-a .a-left {
  width: 50%;
  padding: 26rpx 50rpx;
  box-sizing: border-box;
  border-right: 2rpx solid #EFEFEF;
  position: relative;
}

.incomeexpenses .ie-wrap .wrap-a .a-left:last-child {
  border-right: 0;
}

.incomeexpenses .ie-wrap .wrap-a .a-left .text {
  font-size: 20rpx;
  color: #999999;
  padding-bottom: 10rpx;
}

.incomeexpenses .ie-wrap .wrap-a .a-left .money {
  display: flex;
  align-items: center;
  width: 100%;
  height: 40rpx;
  overflow: hidden;
  position: relative;
  font-size: 24rpx;
  color: #333333;
}

.incomeexpenses .ie-wrap .wrap-a .a-left .icon {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 48rpx;
  right: 48rpx;
  overflow: hidden;
}
.incomeexpenses .ie-wrap .wrap-a .a-left .icon-wrap{
  width: 100%;
  height: 48rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.top{
  width: 24rpx;
  height: 24rpx;
  background: url('data:image/svg+xml;base64,') no-repeat;
  top: -24rpx;
  position: absolute;
  left: 0;
  transition: all 0.5s;
}
.bot{
  width: 24rpx;
  height: 24rpx;
  background: url('data:image/svg+xml;base64,') no-repeat;
  top: 24rpx;
  position: absolute;
  left: 0;
  transition: all 0.5s;
}
/* 绿色箭头 */
/* 金额为正数时 */
.imgScrollTop_neg{
  top: -24rpx;
}
/* 金额为负数时 */
.imgScrollBot_neg{
  top: 0;
}
/* 红色箭头 */
/* 金额为正数时 */
.imgScrollTop_pos{
  top: 0;
}
/* 金额为负数时 */
.imgScrollBot_pos{
  top: 24rpx;
}
.incomeexpenses .ie-wrap .wrap-a .a-left .icon image {
  width: 24rpx;
  height: 24rpx;
}

.discounts {
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
  /* margin-bottom: 20rpx; */
}

.discounts .zone {
  width: 100%;
  height: 100rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 2rpx solid #EFEFEF;
}

.discounts .zone .z-left {
  width: 60%;
  display:flex;
  align-items: center;
}

.discounts .zone .z-left .text{
  height: 34rpx;
  line-height: 1.5;
  font-size: 24rpx;
  color: #333333;
  padding-right: 24rpx;
}

.discounts .zone .z-left .fall{
  display: flex;
  flex: 1;
  height: 100rpx;
  overflow: hidden;
  position: relative;
}

.discounts .zone .z-left .fall .ing{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}

.discounts .zone .z-left .fall .ing .icon {
  width: 34rpx;
  height: 34rpx;
  margin-right: 14rpx;
  border-radius:100%; 
  overflow: hidden;
}

.discounts .zone .z-left .fall .ing .icon image {
  width: 100%;
  height: 100%;
}

.discounts .zone .z-left .fall .ing .xxx{
  width: auto;
  height: 34rpx;
  line-height: 1.5;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #999999;
}
.discounts .zone .z-left .fall .ing .xxx .name {
  width: auto;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.discounts .zone .z-right {
  width: 166rpx;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  color: #999999;
  font-size: 20rpx;
  border: 2rpx solid #E2E2E2;
  border-radius: 26rpx;
}

.discounts .roll {
  width: 100%;
  height: 320rpx;
  margin: 26rpx 0;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
  white-space: nowrap;
}

.discounts .roll .goods{
  width: 200rpx;
  display: inline-block;
  margin-right: 10rpx;
}

.discounts .roll .goods:last-child {
  margin-right: 0;
}

.discounts .roll .goods .img {
  width: 200rpx;
  height: 200rpx;
}

.discounts .roll .goods .img image {
  width: 100%;
  height: 100%;
}

.discounts .roll .goods .name {
  font-size: 20rpx;
  color: #999999;
}

.discounts .roll .goods .money {
  font-size: 24rpx;
  color: #000;
  padding: 8rpx 0;
}

.discounts .roll .goods .name, .discounts .roll .goods .money {
  width: 100%;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
